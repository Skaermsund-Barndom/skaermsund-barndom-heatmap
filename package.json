{
	"browserslist": [
		"defaults and not dead",
		"> 0.5% in alt-EU, last 2 versions"
	],
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/netlify": "6.2.5",
		"@astrojs/solid-js": "^5.0.7",
		"@biomejs/biome": "^1.9.4",
		"@tailwindcss/vite": "^4.1.3",
		"@types/bun": "^1.2.9",
		"astro": "5.6.1",
		"motion": "^12.6.3",
		"netlify-cli": "^20.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"solid-js": "^1.9.5",
		"tailwindcss": "^4.1.3",
		"typescript": "^5.8.3"
	},
	"name": "skaermsund-barndom-heatmap",
	"packageManager": "bun@1.2.9",
	"prettier": {
		"experimentalOperatorPosition": "start",
		"experimentalTernaries": true,
		"overrides": [
			{
				"files": "*.astro",
				"options": {
					"parser": "astro"
				}
			}
		],
		"plugins": [
			"prettier-plugin-astro",
			"prettier-plugin-tailwindcss"
		],
		"useTabs": true
	},
	"private": true,
	"scripts": {
		"astro": "astro",
		"build": "netlify build",
		"build:astro": "bun run check && astro build",
		"check": "astro check",
		"deploy": "bun run build && bun run deploy:netlify",
		"deploy:netlify": "netlify deploy --prod",
		"dev": "astro dev",
		"dev:astro": "astro dev",
		"netlify": "netlify",
		"preview": "bun run build && astro preview",
		"update:deps": "bunx npm-check-updates -u --format group -i"
	},
	"type": "module",
	"version": "1.0.0"
}